<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.codesolusi"
    android:versionCode="1"
    android:versionName="1.0" >
<!-- 	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> -->
    
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light.NoTitleBar" >
        <activity
            android:name="com.example.codesolusi.SplashActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.codesolusi.HomeActivity"></activity>
        <activity android:name="com.example.codesolusi.MainActivity"></activity>
        <activity android:name="com.example.codesolusi.activity_webview"></activity>
        <activity android:name="com.example.codesolusi.TentangActivity"></activity>
        <activity android:name="com.example.codesolusi.ReferensiActivity"></activity>
    </application>

</manifest>
